// Function to update cities dropdown based on selected state
function updateCities() {
    var state = document.getElementById("state").value;
    var cityDropdown = document.getElementById("city");

    // Clear existing options
    cityDropdown.innerHTML = "<option value=''>Select City</option>";

    // Show the city dropdown if a state is selected
    if (state) {
        cityDropdown.style.display = "block"; // Show the city dropdown

        // Get the cities for the selected state
        var cities = states[state];

        // Populate cities for the selected state
        cities.forEach(function(city) {
            var option = document.createElement("option");
            option.value = city;
            option.text = city;
            cityDropdown.appendChild(option);
        });
    } else {
        // Hide the city dropdown if no state is selected
        cityDropdown.style.display = "none";
    }
}

